{
  "expo": {
    "name": "LadderIt",
    "slug": "LadderIt",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/LadderLogo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/LadderLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#9600BC"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ladderit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/LadderLogo.png",
        "backgroundColor": "#9600BC"
      },
      "package": "com.ladderit"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
